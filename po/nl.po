#: ../client/pk-console.c:632
msgid "Percentage"
msgstr "Percentage"

#. TRANSLATORS: the status of the transaction (e.g. downloading)
#: ../client/pk-console.c:650
msgid "Status"
msgstr "Status"

#. TRANSLATORS: the results from the transaction
#: ../client/pk-console.c:679
msgid "Results:"
msgstr "Resultaten:"

