#. TRANSLATORS: this is an atomic transaction
#. TRANSLATORS: the role is the point of the transaction, e.g. update-packages
#: ../client/pk-console.c:174 ../client/pk-console.c:587
msgid "Transaction"
msgstr "Transakcija"

#. TRANSLATORS: this is the time the transaction was started in system
#. timezone
#: ../client/pk-console.c:176
msgid "System time"
msgstr "Sistemsko vreme"

#. TRANSLATORS: this is if the transaction succeeded or not
#: ../client/pk-console.c:178
msgid "Succeeded"
msgstr "Uspela"

#: ../client/pk-console.c:178
msgid "True"
msgstr "Tačno"

#: ../client/pk-console.c:178
msgid "False"
msgstr "Netačno"

#. TRANSLATORS: this is the transactions role, e.g. "update-packages"
#: ../client/pk-console.c:180
msgid "Role"
msgstr "Radnja"

#. TRANSLATORS: this is The duration of the transaction
#: ../client/pk-console.c:185
msgid "Duration"
msgstr "Trajanje"

#: ../client/pk-console.c:185
msgid "(seconds)"
msgstr "(sekundi)"

#. TRANSLATORS: this is The command line used to do the action
#: ../client/pk-console.c:189
msgid "Command line"
msgstr "Komandna linija"

