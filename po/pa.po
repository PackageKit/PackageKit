msgid "Transaction"
msgstr "ਟਰਾਂਸੈਕਸ਼ਨ"

#. TRANSLATORS: this is the time the transaction was started in system timezone
#: client/pkcon/pk-console.c:167
msgid "System time"
msgstr "ਸਿਸਟਮ ਸਮਾਂ"

#: client/pkcon/pk-console.c:169 client/pkgc-util.c:1005
msgid "False"
msgstr "ਗਲਤ"

